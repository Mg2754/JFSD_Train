package com.Training.Day2.hands_on;

class Animal{
	Animal(){
		System.out.println(" jj ");
		
	}
	void makeSound() {
		System.out.println("Sound oF aniaml");
	}
}
class Cat extends Animal{
	Cat(){
		super();
	}
	void makeSound() {
		
		System.out.println("Bark");
	}
}

public class Q1 {
	public static void main(String args[]) {
		Animal obj = new Cat();
		obj.makeSound();
	}

}
